\&.\&.\&.                                  {\*(AqmethodName\*(Aq:\*(Aqaria2\&.addTorrent\*(Aq,
\&.\&.\&.                                   \*(Aqparams\*(Aq:[base64\&.b64encode(open(\*(Aqfile\&.torrent\*(Aq)\&.read())]}]]})
